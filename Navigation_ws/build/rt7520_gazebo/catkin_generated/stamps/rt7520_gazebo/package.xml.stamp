<?xml version="1.0"?>
<package>
  <name>rt7520_gazebo</name>
  <version>0.1.0</version>
  <description>
    Gazebo simulation package for the 7520 Reach Truck agv base
  </description>
  <author email="mrasmussen@bastiansolutions.com">Marq</author>
  <maintainer email="mrasmussen@bastiansolutions.com">Marq</maintainer>

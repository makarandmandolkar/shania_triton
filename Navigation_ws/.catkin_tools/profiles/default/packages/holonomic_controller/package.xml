	<description>Bastian Solutions holonomic controller</description>

  <maintainer email="carowe@bastiansolutions.com">Marq Rasmussen</maintainer>

  <license>BSD</license>
	<author email="carowe@bastiansolutions.com">Marq Rasmussen</author>


  <buildtool_depend>catkin</buildtool_depend>

